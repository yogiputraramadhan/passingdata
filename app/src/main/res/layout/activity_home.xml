<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/logo_hitam"
        android:id="@+id/menu_gojek"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="50dp"
        />
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/menu_gosend"
        android:layout_marginTop="50dp"
        android:id="@+id/menu_gosend"
        app:layout_constraintTop_toBottomOf="@id/menu_gojek"
        app:layout_constraintRight_toLeftOf="@id/menu_goride"
        app:layout_constraintBottom_toTopOf="@id/menu_gomart"
        android:background="#FFFFFF" />
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/menu_goride"
        android:src="@drawable/menu_goride"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@id/menu_gojek"
        app:layout_constraintLeft_toRightOf="@id/menu_gosend"
        app:layout_constraintBottom_toTopOf="@id/menu_gofood"
        android:background="#FFFFFF"
        />

    <ImageButton
        android:id="@+id/menu_gofood"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/menu_gofood"
       app:layout_constraintTop_toBottomOf="@id/menu_goride"
        app:layout_constraintLeft_toRightOf="@id/menu_gomart"
        android:background="#FFFFFF"/>

    <ImageButton
        android:id="@+id/menu_gomart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/menu_gomart"
        android:background="#FFFFFF"
        app:layout_constraintTop_toBottomOf="@id/menu_gosend"
        app:layout_constraintRight_toLeftOf="@id/menu_gofood"/>

</androidx.constraintlayout.widget.ConstraintLayout>